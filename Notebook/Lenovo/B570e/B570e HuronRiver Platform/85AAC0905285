    Scope (_SB.PCI0.LPCB.EC0)
    {
        OperationRegion (GPIO, SystemIO, 0x1180, 0x60)
        Field (GPIO, ByteAcc, Lock, Preserve)
        {
            Offset (0x0C), 
            Offset (0x0E), 
            PO16,   1, 
            PI17,   1, 
            Offset (0x38), 
                ,   4, 
            PO36,   1, 
            PI37,   1, 
            PO38,   1, 
                ,   13, 
            PO52,   1, 
            PO53,   1
        }

        OperationRegion (CMOS, SystemIO, 0x72, 0x02)
        Field (CMOS, ByteAcc, Lock, Preserve)
        {
            INDX,   8, 
            DATA,   8
        }

        IndexField (INDX, DATA, ByteAcc, Lock, Preserve)
        {
            R00H,   8, 
            R01H,   8, 
            R02H,   8, 
            R03H,   8, 
            R04H,   8, 
            R05H,   8, 
            R06H,   8, 
            R07H,   8, 
            R08H,   8, 
            R09H,   8, 
            R0AH,   8, 
            R0BH,   8, 
            R0CH,   8, 
            R0DH,   8, 
            R0EH,   8, 
            R0FH,   8
        }

        IndexField (INDX, DATA, ByteAcc, Lock, Preserve)
        {
            R00W,   16, 
            R02W,   16, 
            R04W,   16, 
            R06W,   16, 
            R08D,   32
        }
    }

    Scope (_SB.PCI0.LPCB.EC0)
    {
        Method (LVKI, 0, NotSerialized)
        {
        }

        Method (_Q5C, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x5C
            Notify (ADP1, 0x80) // Status Change
            VP1D |= 0x04
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q28, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x28
            If (One)
            {
                VP2D |= 0x20
                Notify (VPC0, 0x80) // Status Change
            }
            Else
            {
            }
        }

        Method (_Q14, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x14
            If ((Zero == OPST))
            {
                OPST = One
            }
            Else
            {
                OPST = Zero
            }

            Notify (^^^PEG0.PEGP, 0xDF) // Hardware-Specific
        }

        Method (_Q22, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x22
            VP1D |= 0x20
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q2B, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x2B
            If ((GWSS == Zero)) {}
            Else
            {
                If (WLPS)
                {
                    WLEN = WRST /* \WRST */
                }

                If (BTPS)
                {
                    BTEN = BTST /* \BTST */
                }

                If (WNPS)
                {
                    WWAN = WNST /* \WNST */
                }
            }

            VP2D |= 0x02
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q2D, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x2D
            If (SGEN)
            {
                If ((^^^GFX0.CGPU == One))
                {
                    ^^^GFX0.GPPO = One
                    ^^^GFX0.HGNC = 0x03
                    ^^^GFX0.GPSS = 0x02
                    Notify (GFX0, 0xD0) // Hardware-Specific
                    Notify (WMI1, 0xD0) // Hardware-Specific
                }
            }
        }

        Method (_Q2E, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x2E
            If (CAMC)
            {
                CAMC = Zero
            }
            Else
            {
                CAMC = One
            }

            VP1D |= 0x80
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q70, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x70
            LAMN (0x7A)
        }

        Method (_Q71, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x71
            LAMN (0x7B)
        }

        Method (_Q72, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x72
            LAMN (0x7C)
        }

        Method (_Q73, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x73
            LAMN (0x7D)
        }

        Method (_Q74, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x74
            LAMN (0x7E)
        }

        Method (_Q75, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x75
            LAMN (0x7F)
        }

        Method (_Q27, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x27
            If ((ASPL == One))
            {
                Local0 = GL02 /* \GL02 */
                Local0 |= 0x80
                GL02 = Local0
            }

            If ((ASPL == Zero))
            {
                Local0 = GL02 /* \GL02 */
                Local0 &= 0x7F
                GL02 = Local0
            }
        }
    }

    Scope (_SB.PCI0.LPCB.EC0)
    {
        Name (VCMD, Zero)
        Name (VDAT, Zero)
        Name (NOVS, Zero)
        Name (VP1D, Zero)
        Name (VP2D, Zero)
        Name (GSPS, Zero)
        Name (APDT, Zero)
        Name (OKAD, Zero)
        Name (ZPOS, Zero)
        Name (AMSV, Zero)
        Device (VPC0)
        {
            Name (_HID, "VPC2004")  // _HID: Hardware ID
            Name (_UID, Zero)  // _UID: Unique ID
            Name (VCFG, 0x000D0400)
            Name (VPCD, Zero)
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                Return (0x0F)
            }

            Method (_CFG, 0, NotSerialized)
            {
                Local0 = 0x00080000
                If ((CENS == One))
                {
                    Local0 |= 0x00800000
                }

                Local0 |= 0x00700000
                If ((WLPS == One))
                {
                    Local0 |= 0x00040000
                }

                If ((BTPS == One))
                {
                    Local0 |= 0x00010000
                }

                If ((WNPS == One))
                {
                    Local0 |= 0x00020000
                }

                If (SGEN)
                {
                    If ((SKUT == Zero))
                    {
                        Local0 |= 0x0100
                    }
                    Else
                    {
                        Local0 |= 0x0500
                        If ((^^^^GFX0.CGPU == One))
                        {
                            Local0 &= 0xFFFFEFFF
                        }
                        Else
                        {
                            Local0 |= 0x1000
                        }
                    }
                }

                Local0 |= 0x02
                Local0 |= 0x04
                Return (Local0)
            }

            Method (APPC, 1, Serialized)
            {
                Return (One)
            }

            Method (DBSL, 0, NotSerialized)
            {
                Return (Package (0x0B)
                {
                    0x64, 
                    0x5C, 
                    0x54, 
                    0x4C, 
                    0x44, 
                    0x3C, 
                    0x34, 
                    0x2C, 
                    0x24, 
                    0x1C, 
                    0x14
                })
            }

            Method (SBSL, 0, NotSerialized)
            {
            }

            Method (STHT, 1, Serialized)
            {
                If (Arg0) {}
                Else
                {
                }
            }

            Method (SHDC, 1, Serialized)
            {
                If (Arg0) {}
                Else
                {
                }
            }

            Method (SVCR, 1, Serialized)
            {
                If ((Arg0 == 0x04))
                {
                    Local4 = GL00 /* \GL00 */
                    Local4 &= 0xFB
                    GL00 = Local4
                    BLTM = One
                }
                Else
                {
                    AMSV = Arg0
                }
            }

            Method (VPCR, 1, Serialized)
            {
                If ((Arg0 == One))
                {
                    VPCD = VCMD /* \_SB_.PCI0.LPCB.EC0_.VCMD */
                }
                ElseIf ((Arg0 == Zero))
                {
                    VPCD = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                }
                Else
                {
                    VPCD = Zero
                }

                Return (VPCD) /* \_SB_.PCI0.LPCB.EC0_.VPC0.VPCD */
            }

            Method (VPCW, 2, Serialized)
            {
                VCMD = One
                If ((Arg0 == One))
                {
                    XCMD (Arg1)
                }
                Else
                {
                    VDAT = Arg1
                }

                VCMD = Zero
                Return (Zero)
            }

            Method (XCMD, 1, Serialized)
            {
                Name (_T_0, Zero)  // _T_x: Emitted by ASL Compiler, x=0-9, A-Z
                _T_0 = Arg0
                If ((_T_0 == 0x10))
                {
                    VDAT = VP1D /* \_SB_.PCI0.LPCB.EC0_.VP1D */
                    VP1D = Zero
                }
                ElseIf ((_T_0 == 0x1A))
                {
                    VDAT = VP2D /* \_SB_.PCI0.LPCB.EC0_.VP2D */
                    VP2D = Zero
                }
                ElseIf ((_T_0 == 0x11))
                {
                    VDAT = BMAC /* \_SB_.PCI0.LPCB.EC0_.BMAC */
                }
                ElseIf ((_T_0 == 0x12))
                {
                    Local0 = (^^^^GFX0.CBLV & 0xFF)
                    Divide (Local0, 0x0A, Local2, Local1)
                    VDAT = Local1
                }
                ElseIf ((_T_0 == 0x13))
                {
                    If ((OSYS <= 0x07D3))
                    {
                        BNAC = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        Local0 = BNVA /* \_SB_.PCI0.LPCB.EC0_.BNVA */
                    }
                    Else
                    {
                        Local0 = (^^^^GFX0.CBLV & 0xFF)
                        Local0 = ((Local0 * 0x0A) + 0x0A)
                    }

                    ^^^^GFX0.SBLX (Local0)
                }
                ElseIf ((_T_0 == 0x14))
                {
                    VDAT = WLEN /* \_SB_.PCI0.LPCB.EC0_.WLEN */
                    WRST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                }
                ElseIf ((_T_0 == 0x15))
                {
                    WLEN = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                    WRST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                }
                ElseIf ((_T_0 == 0x16))
                {
                    VDAT = BTEN /* \_SB_.PCI0.LPCB.EC0_.BTEN */
                    BTST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                }
                ElseIf ((_T_0 == 0x17))
                {
                    BTEN = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                    BTST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                }
                ElseIf ((_T_0 == 0x18))
                {
                    VDAT = BLST /* \_SB_.PCI0.LPCB.EC0_.BLST */
                }
                ElseIf ((_T_0 == 0x33))
                {
                    SWBL = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                }
                ElseIf ((_T_0 == 0x19))
                {
                    VDAT = NOVS /* \_SB_.PCI0.LPCB.EC0_.NOVS */
                }
                ElseIf ((_T_0 == 0x1B))
                {
                    VDAT = TPEN /* \_SB_.PCI0.LPCB.EC0_.TPEN */
                }
                ElseIf ((_T_0 == 0x1C)) {}
                ElseIf ((_T_0 == 0x1D))
                {
                    VDAT = CAMC /* \_SB_.PCI0.LPCB.EC0_.CAMC */
                }
                ElseIf ((_T_0 == 0x1E))
                {
                    CAMC = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                }
                ElseIf ((_T_0 == 0x1F))
                {
                    VDAT = WWAN /* \_SB_.PCI0.LPCB.EC0_.WWAN */
                    WNST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                }
                ElseIf ((_T_0 == 0x20))
                {
                    WWAN = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                    WNST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                }
                ElseIf ((_T_0 == 0x21))
                {
                    VDAT = ZPOS /* \_SB_.PCI0.LPCB.EC0_.ZPOS */
                    ZPOS = Zero
                }
                ElseIf ((_T_0 == 0x23))
                {
                    VDAT = GWSS /* \_SB_.PCI0.LPCB.EC0_.GWSS */
                }
                ElseIf ((_T_0 == 0x24))
                {
                    BTEN = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                    BTST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                    WWAN = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                    WNST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                    WLEN = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                    WRST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                }
                ElseIf ((_T_0 == 0x25))
                {
                    VDAT = One
                }
                ElseIf ((_T_0 == 0x27))
                {
                    VDAT = CBST /* \_SB_.PCI0.LPCB.EC0_.CBST */
                }
                ElseIf ((_T_0 == 0x29))
                {
                    VDAT = GSPS /* \_SB_.PCI0.LPCB.EC0_.GSPS */
                }
                ElseIf ((_T_0 == 0x2F))
                {
                    If ((RSBM == Zero))
                    {
                        Local0 = 0x04
                    }

                    If ((AMSV == 0x02))
                    {
                        Local0 &= 0xFD
                    }
                    ElseIf ((AMSV == 0x03))
                    {
                        If (((ICPU >= 0x11) && (ICPU <= 0x22)))
                        {
                            Local0 |= 0x02
                        }
                        Else
                        {
                            Local0 &= 0xFD
                        }
                    }

                    VDAT = Local0
                }
                ElseIf ((_T_0 == 0x31))
                {
                    VDAT = OKAD /* \_SB_.PCI0.LPCB.EC0_.OKAD */
                    OKAD = Zero
                }
                Else
                {
                    Noop
                }
            }
        }

        Method (_Q29, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x29
            NOVS = One
            VP1D |= 0x08
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q76, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x76
            NOVS = Zero
            VP1D |= 0x08
            Notify (VPC0, 0x80) // Status Change
        }

        Method (MNOV, 0, NotSerialized)
        {
            LAMN (0x76)
        }

        Method (_Q2F, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x2F
            VP2D |= 0x04
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q77, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x77
            VP2D |= 0x08
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q19, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x19
            If ((OSYS <= 0x07D2))
            {
                HKDS (0x10)
            }
            Else
            {
                VP1D |= 0x40
                Notify (VPC0, 0x80) // Status Change
            }
        }

        Method (_Q66, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x66
            OKAD |= 0x10
            VP1D |= One
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q67, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x67
            VP1D |= 0x02
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q69, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x69
            OKAD |= 0x08
            VP1D |= One
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q6A, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x6A
            OKAD |= 0x02
            VP1D |= One
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q21, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x21
            ZPOS = One
            Sleep (0x14)
            OPSE = One
            Local0 = GL08 /* \GL08 */
            Local0 &= 0x10
            If ((Local0 == 0x10))
            {
                ODPS = 0x88
            }
            Else
            {
                GL08 |= 0x10
                Notify (SAT0, 0x84) // Reserved
                ODPS = 0x56
            }

            VP2D |= 0x80
            Notify (VPC0, 0x80) // Status Change
            ZPOS = 0x03
        }
    }

    Scope (_SB.PCI0.LPCB.EC0.VPC0)
    {
        OperationRegion (ERAM, EmbeddedControl, Zero, 0xFF)
        Field (RAM, ByteAcc, Lock, Preserve)
        {
            Offset (0x58), 
            ALSD,   8
        }

        Method (HALS, 0, NotSerialized)
        {
            Local0 = ALSD /* \_SB_.PCI0.LPCB.EC0_.VPC0.ALSD */
            Return (Local0)
        }

        Method (SALS, 1, Serialized)
        {
            If ((Arg0 == Zero))
            {
                Local0 = ALSD /* \_SB_.PCI0.LPCB.EC0_.VPC0.ALSD */
                Local0 &= 0xFE
                ALSD = Local0
            }

            If ((Arg0 == One))
            {
                Local0 = ALSD /* \_SB_.PCI0.LPCB.EC0_.VPC0.ALSD */
                Local0 |= One
                ALSD = Local0
            }

            If ((Arg0 == 0x04))
            {
                If ((CENS == One))
                {
                    GL00 &= 0xDF
                    GL02 &= 0xBF
                }
            }

            If ((Arg0 == 0x05))
            {
                If ((CENS == One))
                {
                    GL00 |= 0x20
                    GL02 |= 0x40
                }
            }
        }
    }

    Scope (_SB.PCI0.LPCB.EC0.VPC0)
    {
        Name (KSBM, Zero)
        Method (GBMD, 0, NotSerialized)
        {
            Local0 = 0x10000000
            If ((RSBM == One))
            {
                Local0 |= One
            }

            If ((SMBM == One))
            {
                Local0 |= 0x20
            }

            If ((MBNG == One))
            {
                Local0 |= 0x08
            }

            If ((SBNG == One))
            {
                Local0 |= 0x10
            }

            If ((LBBM == One))
            {
                Local0 |= 0x80
            }

            If ((BNBM == One))
            {
                Local0 |= 0x0100
            }

            If ((CSBM == One))
            {
                Local0 |= 0x0200
            }

            If ((OPBM == One))
            {
                Local0 |= 0x0400
            }

            If ((ROBM == One))
            {
                Local0 |= 0x0800
            }

            Return (Local0)
        }

        Method (SBMC, 1, NotSerialized)
        {
            If ((Arg0 == Zero))
            {
                RSBM = Zero
            }

            If ((Arg0 == One))
            {
                RSBM = One
            }

            If ((Arg0 == 0x02)) {}
            If ((Arg0 == 0x03)) {}
            If ((Arg0 == 0x04))
            {
                SMBM = One
            }

            If ((Arg0 == 0x05))
            {
                SMBM = Zero
                RSBM = Zero
            }

            If ((Arg0 == 0x06)) {}
        }
    }

    Scope (_SB.PCI0.LPCB.EC0.VPC0)
    {
        Method (HODD, 0, NotSerialized)
        {
            If ((SZPO == Zero))
            {
                ZPOF = Zero
                Return (0xFF)
            }
            Else
            {
                ZPOF = One
            }

            Local1 = GL02 /* \GL02 */
            Local1 &= One
            If ((Local1 == One))
            {
                Return (0xFF)
            }
            Else
            {
                Local0 = GL08 /* \GL08 */
                Local0 &= 0x10
                If ((Local0 == 0x10))
                {
                    Return (One)
                }
                Else
                {
                    Return (Zero)
                }
            }
        }

        Method (SODD, 1, Serialized)
        {
            If (Arg0)
            {
                ZPOS = 0x02
                OPEH = One
                Sleep (0x14)
                GL08 |= 0x10
                Notify (SAT0, 0x84) // Reserved
            }
            Else
            {
                GL08 &= 0xEF
            }
        }
    }

    Name (_S0, Package (0x04)  // _S0_: S0 System State
    {
        Zero, 
        Zero, 
        Zero, 
        Zero
    })
    If (SS1)
    {
        Name (_S1, Package (0x04)  // _S1_: S1 System State
        {
            One, 
            Zero, 
            Zero, 
            Zero
        })
    }

    If (SS3)
    {
        Name (_S3, Package (0x04)  // _S3_: S3 System State
        {
            0x05, 
            Zero, 
            Zero, 
            Zero
        })
    }

    If (SS4)
    {
        Name (_S4, Package (0x04)  // _S4_: S4 System State
        {
            0x06, 
            Zero, 
            Zero, 
            Zero
        })
    }

    Name (_S5, Package (0x04)  // _S5_: S5 System State
    {
        0x07, 
        Zero, 
        Zero, 
        Zero
    })
    Method (PTS, 1, NotSerialized)
    {
        If (Arg0) {}
    }

    Method (WAK, 1, NotSerialized)
    {
    }
}



