                /* 0030 */  0x66, 0x69, 0x65, 0x64, 0x20, 0x47, 0x53, 0x79,  // fied GSy
                /* 0038 */  0x6E, 0x63, 0x20, 0x52, 0x65, 0x61, 0x64, 0x79,  // nc Ready
                /* 0040 */  0x20, 0x50, 0x6C, 0x61, 0x74, 0x66, 0x6F, 0x72,  //  Platfor
                /* 0048 */  0x6D, 0x20, 0x66, 0x6F, 0x72, 0x20, 0x41, 0x53,  // m for AS
                /* 0050 */  0x55, 0x53, 0x20, 0x42, 0x75, 0x67, 0x20, 0x32,  // US Bug 2
                /* 0058 */  0x30, 0x30, 0x32, 0x31, 0x34, 0x32, 0x39, 0x34,  // 00214294
                /* 0060 */  0x20, 0x20, 0x20, 0x2D, 0x20, 0x5C, 0x5C, 0x33,  //    - \\3
                /* 0068 */  0x37, 0x3A, 0x57, 0x5D, 0x32, 0x42, 0x33, 0x58,  // 7:W]2B3X
                /* 0070 */  0x46, 0x2E, 0x55, 0x56, 0x29, 0x36, 0x2A, 0x54,  // F.UV)6*T
                /* 0078 */  0x28, 0x5E, 0x59, 0x34, 0x5E, 0x2B, 0x5E, 0x45,  // (^Y4^+^E
                /* 0080 */  0x57, 0x35, 0x26, 0x53, 0x39, 0x2C, 0x4E, 0x25,  // W5&S9,N%
                /* 0088 */  0x2C, 0x29, 0x3D, 0x43, 0x25, 0x20, 0x2D, 0x20,  // ,)=C% - 
                /* 0090 */  0x43, 0x6F, 0x70, 0x79, 0x72, 0x69, 0x67, 0x68,  // Copyrigh
                /* 0098 */  0x74, 0x20, 0x32, 0x30, 0x31, 0x34, 0x20, 0x4E,  // t 2014 N
                /* 00A0 */  0x56, 0x49, 0x44, 0x49, 0x41, 0x20, 0x43, 0x6F,  // VIDIA Co
                /* 00A8 */  0x72, 0x70, 0x6F, 0x72, 0x61, 0x74, 0x69, 0x6F,  // rporatio
                /* 00B0 */  0x6E, 0x20, 0x41, 0x6C, 0x6C, 0x20, 0x52, 0x69,  // n All Ri
                /* 00B8 */  0x67, 0x68, 0x74, 0x73, 0x20, 0x52, 0x65, 0x73,  // ghts Res
                /* 00C0 */  0x65, 0x72, 0x76, 0x65, 0x64, 0x2D, 0x35, 0x38,  // erved-58
                /* 00C8 */  0x39, 0x36, 0x38, 0x34, 0x30, 0x32, 0x39, 0x33,  // 96840293
                /* 00D0 */  0x38, 0x35, 0x28, 0x52, 0x29                     // 85(R)
            })
            Method (_DSM, 4, Serialized)  // _DSM: Device-Specific Method
            {
                If ((Arg0 == ToUUID ("d4a50b75-65c7-46f7-bfb7-41514cea0244")))
                {
                    Switch (ToInteger (Arg2))
                    {
                        Case (Zero)
                        {
                            Return (Buffer (0x04)
                            {
                                 0x21, 0x00, 0x11, 0x00                           // !...
                            })
                        }
                        Case (0x05)
                        {
                            Local1 = (ToInteger (Arg3) & 0x0FFF)
                            Local2 = (ToInteger (Arg3) & 0x00FFF000)
                            Local2 >>= 0x0C
                            If ((CHA1 == One))
                            {
                                CHA3 = One
                                AVLD = Local1
                                ACTD = Local2
                            }

                            CHA1 = Zero
                            CHA2 = Zero
                            Return (Zero)
                        }
                        Case (0x10)
                        {
                            CreateWordField (Arg3, 0x02, BFF0)
                            If ((BFF0 == 0x564B))
                            {
                                If ((0xAF06109B == LCDV))
                                {
                                    Return (GVK0) /* \_SB_.PCI0.PEG0.GFX0.GVK0 */
                                }

                                If ((0xE43004E8 == LCDV))
                                {
                                    Return (GVK1) /* \_SB_.PCI0.PEG0.GFX0.GVK1 */
                                }

                                If ((0xAF06119D == LCDV))
                                {
                                    Return (GVK2) /* \_SB_.PCI0.PEG0.GFX0.GVK2 */
                                }
                            }

                            If ((BFF0 == 0x4452))
                            {
                                If ((0xAF06109B == LCDV))
                                {
                                    Return (GDR0) /* \_SB_.PCI0.PEG0.GFX0.GDR0 */
                                }

                                If ((0xE43004E8 == LCDV))
                                {
                                    Return (GDR1) /* \_SB_.PCI0.PEG0.GFX0.GDR1 */
                                }

                                If ((0xAF06119D == LCDV))
                                {
                                    Return (GDR2) /* \_SB_.PCI0.PEG0.GFX0.GDR2 */
                                }
                            }
                        }
                        Case (0x14)
                        {
                            If ((MSOS () >= OSW8))
                            {
                                If ((LCDS == Zero))
                                {
                                    Return (Package (0x0C)
                                    {
                                        0x8000A430, 
                                        0x0200, 
                                        Zero, 
                                        Zero, 
                                        One, 
                                        One, 
                                        0x03E8, 
                                        0x6E, 
                                        0x03E8, 
                                        Zero, 
                                        Zero, 
                                        Zero
                                    })
                                }
                                Else
                                {
                                    Return (Package (0x0C)
                                    {
                                        0x8000A430, 
                                        0x0203, 
                                        Zero, 
                                        Zero, 
                                        One, 
                                        One, 
                                        0x03E8, 
                                        0x6E, 
                                        0x03E8, 
                                        Zero, 
                                        Zero, 
                                        Zero
                                    })
                                }
                            }

                            Return (Package (0x09)
                            {
                                0x8000A430, 
                                0x0100, 
                                Zero, 
                                Zero, 
                                One, 
                                One, 
                                0xE3, 
                                Zero, 
                                0x03E8
                            })
                        }
                        Default
                        {
                        }

                    }
                }

                If ((Arg0 == ToUUID ("a3132d01-8cda-49ba-a52e-bc9d46df6b81")))
                {
                    Return (GPS (Arg0, Arg1, Arg2, Arg3))
                }

                If ((Arg0 == ToUUID ("cbeca351-067b-4924-9cbd-b46b00b86f34")))
                {
                    Return (NVJT (Arg0, Arg1, Arg2, Arg3))
                }
            }

            Method (_ROM, 2, NotSerialized)  // _ROM: Read-Only Memory
            {
                If (RBUF)
                {
                    Local0 = (Arg0 + RBUF) /* \_SB_.RBUF */
                    Local1 = (Arg1 << 0x03)
                    Name (VBUF, Buffer (Arg1){})
                    OperationRegion (VROM, SystemMemory, Local0, 0x1000)
                    Field (VROM, ByteAcc, NoLock, Preserve)
                    {
                        ROMI,   32768
                    }

                    VBUF = ROMI /* \_SB_.PCI0.PEG0.GFX0._ROM.ROMI */
                    Return (VBUF) /* \_SB_.PCI0.PEG0.GFX0._ROM.VBUF */
                }
                Else
                {
                    Return (Buffer (One)
                    {
                         0x00                                             // .
                    })
                }
            }

            Method (NINT, 2, NotSerialized)
            {
                If ((PBCC == One))
                {
                    If ((Arg0 == One))
                    {
                        PBMX = One
                        Local0 = Zero
                        While ((Local0 < 0x03E8))
                        {
                            PBCM = 0xC5100000
                            Stall (0x64)
                            Local1 = (PBCM & 0xFF)
                            Local0++
                            If ((Local1 == One))
                            {
                                Local0 = 0x03E8
                            }
                        }

                        PBCM = Zero
                        PBMX = Zero
                        P80H = Local1
                        Local2 = PBDO /* \_SB_.PCI0.PEG0.GFX0.PBDO */
                        Return (Local2)
                    }

                    If ((Arg0 == 0x02))
                    {
                        Local2 = Zero
                        If ((PBCM == Zero))
                        {
                            If ((PBMX == Zero))
                            {
                                Local0 = Zero
                                Local3 = (ToInteger (Arg1) * 0x03E8)
                                Divide (Local3, 0xFF, Local2, Local4)
                                Local4 <<= 0x08
                                PBDI = Local4
                                Sleep (0x0A)
                                PBMX = One
                                Sleep (0x0A)
                                PBCM = 0x86100000
                                Sleep (0x0A)
                                While ((Local2 != One))
                                {
                                    Stall (0x64)
                                    Local2 = (PBCM & 0xFF)
                                }

                                PBCM = Zero
                                PBMX = Zero
                            }
                        }

                        Return (Zero)
                    }

                    Return (Zero)
                }
            }

            Method (UCMP, 2, NotSerialized)
            {
                Local1 = SizeOf (Arg0)
                If ((Local1 != SizeOf (Arg1)))
                {
                    Return (Zero)
                }

                Local0 = Zero
                While ((Local0 < Local1))
                {
                    If ((DerefOf (Arg0 [Local0]) != DerefOf (Arg1 [Local0]
                        )))
                    {
                        Return (Zero)
                    }

                    Local0++
                }

                Return (One)
            }

            OperationRegion (GPPB, PCI_Config, 0xB4, 0x14)
            Field (GPPB, ByteAcc, NoLock, Preserve)
            {
                PBCI,   8, 
                PBNP,   8, 
                PBLS,   8, 
                PBCC,   8, 
                PBCM,   32, 
                PBDI,   32, 
                PBDO,   32, 
                PBMX,   32
            }

            Name (SWIT, Zero)
            Name (DDDS, Buffer (0x14){})
            CreateDWordField (DDDS, Zero, D0ST)
            CreateDWordField (DDDS, 0x04, D1ST)
            CreateDWordField (DDDS, 0x08, D2ST)
            CreateDWordField (DDDS, 0x0C, D3ST)
            CreateDWordField (DDDS, 0x10, D4ST)
            CreateWordField (DDDS, Zero, D0ID)
            CreateBitField (DDDS, 0x12, D0EN)
            CreateBitField (DDDS, 0x14, D0CN)
            CreateWordField (DDDS, 0x04, D1ID)
            CreateBitField (DDDS, 0x32, D1EN)
            CreateBitField (DDDS, 0x34, D1CN)
            CreateWordField (DDDS, 0x08, D2ID)
            CreateBitField (DDDS, 0x52, D2EN)
            CreateBitField (DDDS, 0x54, D2CN)
            CreateWordField (DDDS, 0x0C, D3ID)
            CreateBitField (DDDS, 0x72, D3EN)
            CreateBitField (DDDS, 0x74, D3CN)
            CreateWordField (DDDS, 0x10, D4ID)
            CreateBitField (DDDS, 0x92, D4EN)
            CreateBitField (DDDS, 0x94, D4CN)
            Method (DDCN, 1, NotSerialized)
            {
                Local0 = (Arg0 & 0xFFFF)
                If ((Local0 == D0ID))
                {
                    Return (D0CN) /* \_SB_.PCI0.PEG0.GFX0.D0CN */
                }
                ElseIf ((Local0 == D1ID))
                {
                    Return (D1CN) /* \_SB_.PCI0.PEG0.GFX0.D1CN */
                }
                ElseIf ((Local0 == D2ID))
                {
                    Return (D2CN) /* \_SB_.PCI0.PEG0.GFX0.D2CN */
                }
                ElseIf ((Local0 == D3ID))
                {
                    Return (D3CN) /* \_SB_.PCI0.PEG0.GFX0.D3CN */
                }
                ElseIf ((Local0 == D4ID))
                {
                    Return (D4CN) /* \_SB_.PCI0.PEG0.GFX0.D4CN */
                }

                Return (Zero)
            }

            Method (DDEN, 1, NotSerialized)
            {
                Local0 = (Arg0 & 0xFFFF)
                If ((Local0 == D0ID))
                {
                    Return (D0EN) /* \_SB_.PCI0.PEG0.GFX0.D0EN */
                }
                ElseIf ((Local0 == D1ID))
                {
                    Return (D1EN) /* \_SB_.PCI0.PEG0.GFX0.D1EN */
                }
                ElseIf ((Local0 == D2ID))
                {
                    Return (D2EN) /* \_SB_.PCI0.PEG0.GFX0.D2EN */
                }
                ElseIf ((Local0 == D3ID))
                {
                    Return (D3EN) /* \_SB_.PCI0.PEG0.GFX0.D3EN */
                }
                ElseIf ((Local0 == D4ID))
                {
                    Return (D4EN) /* \_SB_.PCI0.PEG0.GFX0.D4EN */
                }

                Return (Zero)
            }

            Method (GNAD, 1, NotSerialized)
            {
                If ((Arg0 == One))
                {
                    Return (0x02)
                }

                If ((Arg0 == 0x02))
                {
                    Return (0x04)
                }

                If ((Arg0 == 0x04))
                {
                    Return (0x08)
                }

                If ((Arg0 == 0x08))
                {
                    Return (0x03)
                }

                If ((Arg0 == 0x03))
                {
                    Return (0x05)
                }

                If ((Arg0 == 0x05))
                {
                    Return (0x09)
                }

                If ((Arg0 == 0x09))
                {
                    Return (0x06)
                }

                If ((Arg0 == 0x06))
                {
                    Return (0x0A)
                }

                If ((Arg0 == 0x0A))
                {
                    Return (0x0C)
                }

                If ((Arg0 == 0x0C))
                {
                    Return (One)
                }

                Return (One)
            }

            Method (DGSM, 0, NotSerialized)
            {
                Local0 = Zero
                If (DDCN (0x0110))
                {
                    Local0 |= One
                }

                If (DDCN (0x80000100))
                {
                    Local0 |= 0x02
                }

                If (DDCN (0x80000200))
                {
                    Local0 |= 0x04
                }

                If (DDCN (0x80007330))
                {
                    Local0 |= 0x08
                }

                Local1 = Zero
                If (DDEN (0x0110))
                {
                    Local1 |= One
                }

                If (DDEN (0x80000100))
                {
                    Local1 |= 0x02
                }

                If (DDEN (0x80000200))
                {
                    Local1 |= 0x04
                }

                If (DDEN (0x80007330))
                {
                    Local1 |= 0x08
                }

                Local3 = 0x0A
                While (Local3)
                {
                    Local1 = GNAD (Local1)
                    Local2 = (Local0 & Local1)
                    If ((Local1 == Local2))
                    {
                        SWIT = Local1
                        Break
                    }

                    Local3--
                }
            }

            Method (OWAK, 1, NotSerialized)
            {
            }

            Method (OPTS, 1, NotSerialized)
            {
            }
        }
    }

    Scope (_SB.PCI0.PEG0.GFX0)
    {
        Name (TGPS, Zero)
        Name (TRPC, Zero)
        Name (GEIF, One)
        Name (GSWF, One)
        Name (LTRE, Zero)
        OperationRegion (GB21, SystemMemory, 0xFDAF0568, 0x04)
        Field (GB21, AnyAcc, NoLock, Preserve)
        {
                ,   19, 
            B21R,   1, 
                ,   3, 
            B21I,   1
        }

        OperationRegion (GPSB, SystemMemory, 0xFDAF0144, 0x04)
        Field (GPSB, AnyAcc, NoLock, Preserve)
        {
                ,   21, 
            B21S,   1
        }

        Method (GC6I, 0, Serialized)
        {
            Debug = "<<< GC6I >>>"
            ^^^LPCB.EC0.WRAM (0x0520, 0x95)
            ^^^LPCB.EC0.WRAM (0x03A4, Zero)
            ^^^LPCB.EC0.WRAM (0x03A5, Zero)
            Local0 = ^^^LPCB.EC0.RRAM (0x045C)
            Local0 |= 0x80
            ^^^LPCB.EC0.WRAM (0x045C, Local0)
            LTRE = LREN /* External reference */
            LNKD = One
            Sleep (One)
            P0RM = One
            P0AP = 0x03
            If ((GSWF == One))
            {
                B21S = One
                B21I = One
                B21R = One
                Stall (0x64)
            }

            TGPS = Zero
        }

        Method (GC6O, 0, Serialized)
        {
            Debug = "<<< GC6O >>>"
            CreateField (TGPC, 0x13, One, EGEI)
            If ((GSWF == One))
            {
                If ((B21R == One))
                {
                    B21R = Zero
                    If ((GGIV (0x01010015) == One))
                    {
                        B21I = Zero
                        B21S = One
                    }
                }
                Else
                {
                    B21I = Zero
                    B21S = One
                    Return (One)
                }
            }

            P0AP = Zero
            P0RM = Zero
            LNKD = Zero
            If ((GGIV (0x01010015) == One))
            {
                SGOV (0x01010014, Zero)
                While ((GGIV (0x01010015) != Zero))
                {
                    Sleep (One)
                }

                SGOV (0x01010014, One)
            }

            B21I = Zero
            B21S = One
            While ((P0LS < 0x03))
            {
                Sleep (One)
            }

            LREN = LTRE /* \_SB_.PCI0.PEG0.GFX0.LTRE */
            CEDR = One
            If ((ToInteger (EGEI) == One))
            {
                OperationRegion (PTMP, SystemMemory, (XBAS + 0x00100000), 0x04)
                Field (PTMP, DWordAcc, NoLock, Preserve)
                {
                    TEMP,   8
                }

                Name (PTM1, Zero)
                PTM1 = TEMP /* \_SB_.PCI0.PEG0.GFX0.GC6O.TEMP */
                TEMP = PTM1 /* \_SB_.PCI0.PEG0.GFX0.GC6O.PTM1 */
            }

            ^^^LPCB.EC0.WRAM (0x052B, 0x9E)
            ^^^LPCB.EC0.WRAM (0x0520, 0x97)
            Local0 = ^^^LPCB.EC0.RRAM (0x045C)
            Local0 &= 0x7F
            ^^^LPCB.EC0.WRAM (0x045C, Local0)
        }

        Method (NVJT, 4, Serialized)
        {
            If ((GGIV (0x01030009) == Zero))
            {
                If ((GGIV (0x0103000A) == One))
                {
                    If ((Arg1 < 0x0100))
                    {
                        Return (0x80000001)
                    }

                    Switch (ToInteger (Arg2))
                    {
                        Case (Zero)
                        {
                            Return (Buffer (0x04)
                            {
                                 0x7F, 0x00, 0x00, 0x00                           // ....
                            })
                        }
                        Case (One)
                        {
                            Name (JTCA, Buffer (0x04)
                            {
                                 0x00                                             // .
                            })
                            CreateField (JTCA, Zero, One, JTEN)
                            CreateField (JTCA, One, 0x02, SREN)
                            CreateField (JTCA, 0x03, 0x02, PLPR)
                            CreateField (JTCA, 0x05, One, SRPR)
                            CreateField (JTCA, 0x06, 0x02, FBPR)
                            CreateField (JTCA, 0x08, 0x02, GUPR)
                            CreateField (JTCA, 0x0A, One, GC6R)
                            CreateField (JTCA, 0x0B, One, PTRH)
                            CreateField (JTCA, 0x0D, One, MHYB)
                            CreateField (JTCA, 0x0E, One, RPCL)
                            CreateField (JTCA, 0x0F, 0x02, GC6V)
                            CreateField (JTCA, 0x11, One, GEIS)
                            CreateField (JTCA, 0x12, One, GSWS)
                            CreateField (JTCA, 0x14, 0x0C, JTRV)
                            JTEN = One
                            GC6R = Zero
                            SREN = Zero
                            PLPR = 0x02
                            SRPR = One
                            MHYB = One
                            RPCL = One
                            If ((ToInteger (RPCL) == One))
                            {
                                TRPC = One
                            }

                            GC6V = One
                            GEIS = GEIF /* \_SB_.PCI0.PEG0.GFX0.GEIF */
                            GSWS = GSWF /* \_SB_.PCI0.PEG0.GFX0.GSWF */
                            JTRV = 0x0103
                            Return (JTCA) /* \_SB_.PCI0.PEG0.GFX0.NVJT.JTCA */
                        }
                        Case (0x02)
                        {
                            Return (0x80000002)
                        }
                        Case (0x03)
                        {
                            CreateField (Arg3, Zero, 0x03, GUPC)
                            CreateField (Arg3, 0x04, One, PLPC)
                            CreateField (Arg3, 0x07, One, ECOC)
                            CreateField (Arg3, 0x0E, 0x02, DFGC)
                            CreateField (Arg3, 0x10, 0x03, GPCX)
                            TGPC = Arg3
                            If (((ToInteger (GUPC) != Zero) || (ToInteger (DFGC
                                ) != Zero)))
                            {
                                TDGC = ToInteger (DFGC)
                                DGCX = ToInteger (GPCX)
                            }

                            Name (JTPC, Buffer (0x04)
                            {
                                 0x00                                             // .
                            })
                            CreateField (JTPC, Zero, 0x03, GUPS)
                            CreateField (JTPC, 0x07, One, PLST)
                            CreateField (JTPC, 0x03, One, GPWO)
                            If ((ToInteger (DFGC) != Zero))
                            {
                                GPWO = One
                                GUPS = One
                                Return (JTPC) /* \_SB_.PCI0.PEG0.GFX0.NVJT.JTPC */
                            }

                            If ((ToInteger (GUPC) == One))
                            {
                                TGPS = Zero
                                GC6I ()
                                PLST = One
                                GUPS = Zero
                            }
                            ElseIf ((ToInteger (GUPC) == 0x02))
                            {
                                TGPS = Zero
                                GC6I ()
                                If ((ToInteger (PLPC) == Zero))
                                {
                                    PLST = Zero
                                }

                                GUPS = Zero
                            }
                            ElseIf ((ToInteger (GUPC) == 0x03))
                            {
                                TGPS = Zero
                                GC6O ()
                                If ((ToInteger (PLPC) != Zero))
                                {
                                    PLST = Zero
                                }

                                GPWO = One
                                GUPS = One
                            }
                            ElseIf ((ToInteger (GUPC) == 0x04))
                            {
                                TGPS = Zero
                                GC6O ()
                                If ((ToInteger (PLPC) != Zero))
                                {
                                    PLST = Zero
                                }

                                GPWO = One
                                GUPS = One
                            }
                            ElseIf ((ToInteger (GUPC) == 0x05))
                            {
                                TGPS = Zero
                                GC6I ()
                                If ((ToInteger (PLPC) == Zero))
                                {
                                    PLST = Zero
                                }

                                GUPS = Zero
                            }
                            ElseIf ((ToInteger (GUPC) == 0x06))
                            {
                                TGPS = Zero
                                GC6O ()
                                If ((ToInteger (PLPC) != Zero))
                                {
                                    PLST = Zero
                                }

                                GPWO = One
                                GUPS = One
                            }
                            ElseIf ((ToInteger (GUPC) == Zero))
                            {
                                If ((GGIV (0x01010015) == Zero))
                                {
                                    GUPS = One
                                }
                                Else
                                {
                                    GUPS = 0x03
                                }

                                If ((ToInteger (GUPS) == One))
                                {
                                    GPWO = One
                                }
                                Else
                                {
                                    GPWO = Zero
                                }
                            }

                            Return (JTPC) /* \_SB_.PCI0.PEG0.GFX0.NVJT.JTPC */
                        }
                        Case (0x04)
                        {
                            Return (0x80000002)
                        }

                    }
                }
                Else
                {
                    Return (0x80000002)
                }
            }
            Else
            {
                Return (0x80000002)
            }
        }
    }

    Scope (_GPE)
    {
        Method (_L55, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            If ((\_SB.PCI0.PEG0.GFX0.B21I == One))
            {
                \_SB.PCI0.PEG0.GFX0.GC6O ()
            }
        }
    }

    Scope (_SB.PCI0)
    {
        Name (GPS, "GPSACPI 2012-Aug-12 14:56:05")
    }

    Scope (_SB.PCI0.PEG0.GFX0)
    {
        Name (PSAP, Zero)
        Name (ECBF, Buffer (0x14){})
        CreateDWordField (ECBF, Zero, EDS1)
        CreateDWordField (ECBF, 0x04, EDS2)
        CreateDWordField (ECBF, 0x08, EDS3)
        CreateDWordField (ECBF, 0x0C, EDS4)
        CreateDWordField (ECBF, 0x10, EPDT)
        Name (GPSP, Buffer (0x24){})
        CreateDWordField (GPSP, Zero, RETN)
        CreateDWordField (GPSP, 0x04, VRV1)
        CreateDWordField (GPSP, 0x08, TGPU)
        CreateDWordField (GPSP, 0x0C, PDTS)
        CreateDWordField (GPSP, 0x10, SFAN)
        CreateDWordField (GPSP, 0x14, SKNT)
        CreateDWordField (GPSP, 0x18, CPUE)
        CreateDWordField (GPSP, 0x1C, TMP1)
        CreateDWordField (GPSP, 0x20, TMP2)
        Name (NLIM, Zero)
        Method (GPS, 4, Serialized)
        {
            Debug = "------- GPS DSM --------"
            If ((Arg1 != 0x0100))
            {
                Return (0x80000002)
            }

            Switch (ToInteger (Arg2))
            {
                Case (Zero)
                {
                    Name (FMSK, Buffer (0x08)
                    {
                         0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF   // ........
                    })
                    Local0 = Buffer (0x08)
                        {
                             0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00   // ........
                        }
                    Divide (Zero, 0x08, Local2, Local1)
                    Local2 = (One << Local2)
                    Local0 [Local1] = (DerefOf (Local0 [Local1]) | Local2)
                    Divide (0x13, 0x08, Local2, Local1)
                    Local2 = (One << Local2)
                    Local0 [Local1] = (DerefOf (Local0 [Local1]) | Local2)
                    Divide (0x20, 0x08, Local2, Local1)
                    Local2 = (One << Local2)
                    Local0 [Local1] = (DerefOf (Local0 [Local1]) | Local2)
                    Divide (0x21, 0x08, Local2, Local1)
                    Local2 = (One << Local2)
                    Local0 [Local1] = (DerefOf (Local0 [Local1]) | Local2)
                    Divide (0x22, 0x08, Local2, Local1)
                    Local2 = (One << Local2)
                    Local0 [Local1] = (DerefOf (Local0 [Local1]) | Local2)
                    Divide (0x23, 0x08, Local2, Local1)
                    Local2 = (One << Local2)
                    Local0 [Local1] = (DerefOf (Local0 [Local1]) | Local2)
                    Divide (0x2A, 0x08, Local2, Local1)
                    Local2 = (One << Local2)
                    Local0 [Local1] = (DerefOf (Local0 [Local1]) | Local2)
                    Local1 = SizeOf (Local0)
                    While ((Local1 != Zero))
                    {
                        Local1--
                        Local2 = DerefOf (FMSK [Local1])
                        Local0 [Local1] = (DerefOf (Local0 [Local1]) & Local2)
                    }

                    Return (Local0)
                }
                Case (0x13)
                {
                    Debug = "GPS fun 19"
                    Return (Arg3)
                }
                Case (0x20)
                {
                    Debug = "GPS fun 20"
                    Name (RET1, Zero)
                    CreateBitField (Arg3, 0x18, NRIT)
                    CreateBitField (Arg3, 0x19, NRIS)
                    If (NRIS)
                    {
                        If (NRIT)
                        {
                            RET1 |= 0x01000000
                        }
                        Else
                        {
                            RET1 &= 0xFEFFFFFF
                        }
                    }

                    RET1 |= 0x40000000
                    If (NLIM)
                    {
                        RET1 |= One
                    }

                    Return (RET1) /* \_SB_.PCI0.PEG0.GFX0.GPS_.RET1 */
                }
                Case (0x21)
                {
                    Return (\_PR.CPU0._PSS) /* External reference */
                }
                Case (0x22)
                {
                    CreateByteField (Arg3, Zero, PCAP)
                    \_PR.CPPC = PCAP /* \_SB_.PCI0.PEG0.GFX0.GPS_.PCAP */
                    PNOT ()
                    PSAP = PCAP /* \_SB_.PCI0.PEG0.GFX0.GPS_.PCAP */
                    Return (PCAP) /* \_SB_.PCI0.PEG0.GFX0.GPS_.PCAP */
                }
                Case (0x23)
                {
                    Return (PSAP) /* \_SB_.PCI0.PEG0.GFX0.PSAP */
                }
                Case (0x25)
                {
                    Debug = "GPS fun 25"
                    Return (\_PR.CPU0._TSS) /* External reference */
                }
                Case (0x26)
                {
                    Debug = "GPS fun 26"
                    CreateDWordField (Arg3, Zero, TCAP)
                    \_PR.CPU0._PTC = TCAP /* \_SB_.PCI0.PEG0.GFX0.GPS_.TCAP */
                    Notify (\_PR.CPU0, 0x80) // Performance Capability Change
                    Return (TCAP) /* \_SB_.PCI0.PEG0.GFX0.GPS_.TCAP */
                }
                Case (0x2A)
                {
                    Debug = "GPS fun 2a"
                    CreateByteField (Arg3, Zero, PSH0)
                    CreateByteField (Arg3, One, PSH1)
                    CreateBitField (Arg3, 0x08, GPUT)
                    CreateBitField (Arg3, 0x09, CPUT)
                    CreateBitField (Arg3, 0x0A, FANS)
                    CreateBitField (Arg3, 0x0B, SKIN)
                    CreateBitField (Arg3, 0x0C, ENGR)
                    CreateBitField (Arg3, 0x0D, SEN1)
                    CreateBitField (Arg3, 0x0E, SEN2)
                    Switch (PSH0)
                    {
                        Case (Zero)
                        {
                            If (CPUT)
                            {
                                RETN = 0x0200
                                RETN |= PSH0 /* \_SB_.PCI0.PEG0.GFX0.GPS_.PSH0 */
                                PDTS = ^^^LPCB.EC0.ECPU /* \_SB_.PCI0.LPCB.EC0_.ECPU */
                            }

                            Return (GPSP) /* \_SB_.PCI0.PEG0.GFX0.GPSP */
                        }
                        Case (One)
                        {
                            RETN = 0x0300
                            RETN |= PSH0 /* \_SB_.PCI0.PEG0.GFX0.GPS_.PSH0 */
                            PDTS = 0x03E8
                            Return (GPSP) /* \_SB_.PCI0.PEG0.GFX0.GPSP */
                        }
                        Case (0x02)
                        {
                            RETN = 0x0102
                            VRV1 = Zero
                            TGPU = ^^^LPCB.EC0.ST8E (0x23, Zero)
                            PDTS = Zero
                            SFAN = Zero
                            CPUE = Zero
                            SKNT = Zero
                            TMP1 = Zero
                            TMP2 = Zero
                            Return (GPSP) /* \_SB_.PCI0.PEG0.GFX0.GPSP */
                        }

                    }
                }

            }

            Return (0x80000002)
        }
    }
}



