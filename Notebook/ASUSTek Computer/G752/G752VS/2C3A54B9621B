    Scope (_SB.PCI0.PEG0)
    {
        Device (GFX0)
        {
            Name (_ADR, Zero)  // _ADR: Address
            Method (_PRW, 0, NotSerialized)  // _PRW: Power Resources for Wake
            {
                Return (GPRW (0x09, 0x04))
            }

            OperationRegion (VSID, PCI_Config, Zero, 0x04)
            Field (VSID, ByteAcc, NoLock, Preserve)
            {
                REG0,   32
            }

            Name (PCTG, Package (0x0B){})
            Method (PRST, 0, NotSerialized)
            {
                If ((REG0 == 0xFFFFFFFF))
                {
                    Return (Zero)
                }
                Else
                {
                    Return (One)
                }
            }

            Name (UPDN, One)
            Name (NXTD, One)
            Name (LCDM, One)
            Name (CRTM, 0x02)
            Name (TVOM, 0x04)
            Name (DVIM, 0x08)
            Name (HDMM, 0x10)
            Name (DPOM, 0x20)
            Name (EDPM, 0x40)
            Name (PACH, Zero)
            Name (DONE, Zero)
            Name (DOSF, One)
            Name (BRNC, Zero)
            Name (CHA1, Zero)
            Name (CHA2, Zero)
            Name (CHA3, One)
            Name (CRTB, Zero)
            Name (LCDB, Zero)
            Name (DPOB, Zero)
            Name (EDPB, Zero)
            Name (HDMB, Zero)
            Name (DVIB, Zero)
            Name (TVOB, Zero)
            Method (CONT, 0, NotSerialized)
            {
                CRTB = One
                Local0 = 0x02
                DPOB = Local0
                Local0 <<= One
                EDPB = Local0
                Local0 <<= One
                HDMB = Local0
                Local0 <<= One
                LCDB = Local0
                Local0 <<= One
            }

            Method (MD2A, 1, NotSerialized)
            {
                Local0 = Zero
                If ((Arg0 & LCDB))
                {
                    Local0 |= One
                }

                If ((Arg0 & CRTB))
                {
                    Local0 |= 0x02
                }

                If ((Arg0 & TVOB))
                {
                    Local0 |= 0x04
                }

                If ((Arg0 & DVIB))
                {
                    Local0 |= 0x08
                }

                If ((Arg0 & HDMB))
                {
                    Local0 |= 0x80
                }

                If ((Arg0 & DPOB))
                {
                    Local0 |= 0x20
                }

                If ((Arg0 & EDPB))
                {
                    Local0 |= 0x40
                }

                Return (Local0)
            }

            Method (_INI, 0, NotSerialized)  // _INI: Initialize
            {
                ^^^^DOSF = DOSF /* \_SB_.PCI0.PEG0.GFX0.DOSF */
                GETD ()
            }

            Method (_DOS, 1, NotSerialized)  // _DOS: Disable Output Switching
            {
                DOSF = (Arg0 & 0x03)
                ^^^^DOSF = DOSF /* \_SB_.PCI0.PEG0.GFX0.DOSF */
                BRNC = (Arg0 >> 0x02)
                BRNC &= One
            }

            Name (DODP, Package (0x05)
            {
                0x00010100, 
                0xFFFF0020, 
                0xFFFF0030, 
                0xFFFF0010, 
                0x00010110
            })
            Method (_DOD, 0, NotSerialized)  // _DOD: Display Output Devices
            {
                If (!PACH)
                {
                    If ((VGAF == 0x02))
                    {
                        DODP [Match (DODP, MEQ, 0xFFFF0020, MTR, Zero, Zero)] = 0x0212
                        DODP [Match (DODP, MEQ, 0xFFFF0010, MTR, Zero, Zero)] = 0x0220
                    }
                    Else
                    {
                        DODP [Match (DODP, MEQ, 0xFFFF0020, MTR, Zero, Zero)] = 0x80006340
                        DODP [Match (DODP, MEQ, 0xFFFF0030, MTR, Zero, Zero)] = 0x8000A430
                        DODP [Match (DODP, MEQ, 0xFFFF0010, MTR, Zero, Zero)] = 0x80008330
                        CONT ()
                    }

                    PACH = One
                }

                Return (DODP) /* \_SB_.PCI0.PEG0.GFX0.DODP */
            }

            Device (CRTD)
            {
                Name (_ADR, 0x0100)  // _ADR: Address
                Method (_DCS, 0, NotSerialized)  // _DCS: Display Current Status
                {
                    Local0 = AVLD /* \_SB_.AVLD */
                    If (Local0)
                    {
                        If ((Local0 & CRTM))
                        {
                            Return (0x1F)
                        }
                    }

                    Return (0x1D)
                }

                Method (_DGS, 0, NotSerialized)  // _DGS: Display Graphics State
                {
                    Local0 = (NXTD & CRTM) /* \_SB_.PCI0.PEG0.GFX0.CRTM */
                    If (Local0)
                    {
                        Return (One)
                    }

                    Return (Zero)
                }

                Method (_DSS, 1, NotSerialized)  // _DSS: Device Set State
                {
                    DONE = One
                }
            }

            Device (DPOD)
            {
                Method (_ADR, 0, NotSerialized)  // _ADR: Address
                {
                    If ((VGAF == 0x02))
                    {
                        Return (0x0210)
                    }
                    Else
                    {
                        Return (0x80006340)
                    }
                }

                Method (_DCS, 0, NotSerialized)  // _DCS: Display Current Status
                {
                    Local0 = AVLD /* \_SB_.AVLD */
                    If (Local0)
                    {
                        If ((Local0 & DPOM))
                        {
                            Return (0x1F)
                        }
                    }

                    Return (0x1D)
                }

                Method (_DGS, 0, NotSerialized)  // _DGS: Display Graphics State
                {
                    Local0 = (NXTD & DPOM) /* \_SB_.PCI0.PEG0.GFX0.DPOM */
                    If (Local0)
                    {
                        Return (One)
                    }

                    Return (Zero)
                }

                Method (_DSS, 1, NotSerialized)  // _DSS: Device Set State
                {
                    DONE = One
                }
            }

            Device (EDPD)
            {
                Method (_ADR, 0, NotSerialized)  // _ADR: Address
                {
                    If ((VGAF == 0x02))
                    {
                        Return (0x0210)
                    }
                    Else
                    {
                        Return (0x8000A430)
                    }
                }

                Method (_DCS, 0, NotSerialized)  // _DCS: Display Current Status
                {
                    CHA1 = One
                    CHA2 = One
                    CHA3 = Zero
                    Notify (GFX0, 0xF0) // Hardware-Specific
                    While ((CHA2 == One))
                    {
                        Sleep (0x32)
                        If ((CHA3 != One))
                        {
                            CHA2 = Zero
                        }
                    }

                    ACTD = MD2A (ACTD)
                    AVLD = MD2A (AVLD)
                    Local0 = AVLD /* \_SB_.AVLD */
                    If (Local0)
                    {
                        If ((Local0 & EDPM))
                        {
                            Return (0x1F)
                        }
                    }

                    Return (0x1D)
                }

                Method (_DGS, 0, NotSerialized)  // _DGS: Display Graphics State
                {
                    Local0 = (NXTD & EDPM) /* \_SB_.PCI0.PEG0.GFX0.EDPM */
                    If (Local0)
                    {
                        Return (One)
                    }

                    Return (Zero)
                }

                Method (_DSS, 1, NotSerialized)  // _DSS: Device Set State
                {
                    DONE = One
                }

                Method (_BCL, 0, NotSerialized)  // _BCL: Brightness Control Levels
                {
                    If ((MSOS () >= OSW8))
                    {
                        Local0 = (0x64 + One)
                        Local0 += 0x02
                        Name (BRPP, Package (0x67){})
                        BRPP [Zero] = 0x64
                        BRPP [One] = 0x28
                        Local1 = 0x02
                        Local2 = Zero
                        While ((Local1 < Local0))
                        {
                            BRPP [Local1] = Local2
                            Local1++
                            Local2++
                        }

                        Local0 = Zero
                        While ((Local0 <= 0x0A))
                        {
                            PCTG [(0x0A - Local0)] = (Local0 * 0x0A)
                            Local0++
                        }

                        Return (BRPP) /* \_SB_.PCI0.PEG0.GFX0.EDPD._BCL.BRPP */
                    }

                    Local0 = Zero
                    Local4 = BRTI /* \_SB_.BRTI */
                    Local4 <<= 0x04
                    While ((Local0 < 0x0B))
                    {
                        Local3 = (0x0B - Local0)
                        Local3 -= One
                        Local3 = (Local4 + Local3)
                        Local1 = DerefOf (^^^^LPCB.EC0.PWAC [Local3])
                        Local2 = ((Local1 * 0x64) / 0xFF)
                        PCTG [Local0] = Local2
                        Local0++
                    }

                    Return (PCTG) /* \_SB_.PCI0.PEG0.GFX0.PCTG */
                }

                Name (BCBH, Zero)
                Method (_BCM, 1, NotSerialized)  // _BCM: Brightness Control Method
                {
                    If ((MSOS () >= OSW8))
                    {
                        LBTN = Arg0
                    }
                    Else
                    {
                        Local0 = GCBL (Arg0)
                        LBTN = (0x0A - Local0)
                    }

                    ^^^^LPCB.EC0.STBR ()
                }

                Method (_BQC, 0, NotSerialized)  // _BQC: Brightness Query Current
                {
                    Return (LBTN) /* \_SB_.LBTN */
                }
            }

            Device (HDMI)
            {
                Method (_ADR, 0, NotSerialized)  // _ADR: Address
                {
                    If ((VGAF == 0x02))
                    {
                        Return (0x0220)
                    }
                    Else
                    {
                        Return (0x80008331)
                    }
                }

                Method (_DCS, 0, NotSerialized)  // _DCS: Display Current Status
                {
                    Local0 = AVLD /* \_SB_.AVLD */
                    If (Local0)
                    {
                        If ((Local0 & HDMM))
                        {
                            Return (0x1F)
                        }
                    }

                    Return (0x1D)
                }

                Method (_DGS, 0, NotSerialized)  // _DGS: Display Graphics State
                {
                    Local0 = (NXTD & HDMM) /* \_SB_.PCI0.PEG0.GFX0.HDMM */
                    If (Local0)
                    {
                        Return (One)
                    }

                    Return (Zero)
                }

                Method (_DSS, 1, NotSerialized)  // _DSS: Device Set State
                {
                    DONE = One
                }
            }

            Device (LCDD)
            {
                Name (_ADR, 0x0110)  // _ADR: Address
                Method (_DCS, 0, NotSerialized)  // _DCS: Display Current Status
                {
                    CHA1 = One
                    CHA2 = One
                    CHA3 = Zero
                    Notify (GFX0, 0xF0) // Hardware-Specific
                    While ((CHA2 == One))
                    {
                        Sleep (0x32)
                        If ((CHA3 != One))
                        {
                            CHA2 = Zero
                        }
                    }

                    ACTD = MD2A (ACTD)
                    AVLD = MD2A (AVLD)
                    Local0 = ACTD /* \_SB_.ACTD */
                    If (Local0)
                    {
                        If ((Local0 & LCDM))
                        {
                            Return (0x1F)
                        }
                    }

                    Return (0x1D)
                }

                Method (_DGS, 0, NotSerialized)  // _DGS: Display Graphics State
                {
                    Local0 = (NXTD & LCDM) /* \_SB_.PCI0.PEG0.GFX0.LCDM */
                    If (Local0)
                    {
                        Return (One)
                    }

                    Return (Zero)
                }

                Method (_DSS, 1, NotSerialized)  // _DSS: Device Set State
                {
                    DONE = One
                }

                Method (_BCL, 0, NotSerialized)  // _BCL: Brightness Control Levels
                {
                    If ((MSOS () >= OSW8))
                    {
                        Local0 = (0x64 + One)
                        Local0 += 0x02
                        Name (BRPP, Package (0x67){})
                        BRPP [Zero] = 0x64
                        BRPP [One] = 0x28
                        Local1 = 0x02
                        Local2 = Zero
                        While ((Local1 < Local0))
                        {
                            BRPP [Local1] = Local2
                            Local1++
                            Local2++
                        }

                        Local0 = Zero
                        While ((Local0 <= 0x0A))
                        {
                            PCTG [(0x0A - Local0)] = (Local0 * 0x0A)
                            Local0++
                        }

                        Return (BRPP) /* \_SB_.PCI0.PEG0.GFX0.LCDD._BCL.BRPP */
                    }

                    Local0 = Zero
                    Local4 = BRTI /* \_SB_.BRTI */
                    Local4 <<= 0x04
                    While ((Local0 < 0x0B))
                    {
                        Local3 = (0x0B - Local0)
                        Local3 -= One
                        Local3 = (Local4 + Local3)
                        Local1 = DerefOf (^^^^LPCB.EC0.PWAC [Local3])
                        Local2 = ((Local1 * 0x64) / 0xFF)
                        PCTG [Local0] = Local2
                        Local0++
                    }

                    Return (PCTG) /* \_SB_.PCI0.PEG0.GFX0.PCTG */
                }

                Name (BCBH, Zero)
                Method (_BCM, 1, NotSerialized)  // _BCM: Brightness Control Method
                {
                    If ((MSOS () >= OSW8))
                    {
                        LBTN = Arg0
                    }
                    Else
                    {
                        Local0 = GCBL (Arg0)
                        LBTN = (0x0A - Local0)
                    }

                    ^^^^LPCB.EC0.STBR ()
                }

                Method (_BQC, 0, NotSerialized)  // _BQC: Brightness Query Current
                {
                    Return (LBTN) /* \_SB_.LBTN */
                }
            }

            Method (SWHD, 1, Serialized)
            {
                UPDN = One
                If ((DOSF == One))
                {
                    SETD = Arg0
                    ISMI (0x94)
                }
                Else
                {
                    SETD = Arg0
                    DONE = Zero
                    Notify (GFX0, 0x80) // Status Change
                    Local0 = 0x01F4
                    While (Local0)
                    {
                        If (DONE)
                        {
                            Local0 = Zero
                        }
                        Else
                        {
                            Sleep (0x0A)
                            Local0--
                        }
                    }
                }
            }

            Method (GETD, 0, NotSerialized)
            {
                Return (ACTD) /* \_SB_.ACTD */
            }

            Method (GNDD, 0, NotSerialized)
            {
                ISMI (0xA0)
                Return (SETD) /* \_SB_.SETD */
            }

            Name (VGAT, Buffer (0x23)
            {
                /* 0000 */  0x01, 0x02, 0x03, 0x04, 0x05, 0x06, 0x08, 0x09,  // ........
                /* 0008 */  0x0A, 0x0C, 0x10, 0x11, 0x12, 0x14, 0x18, 0x20,  // ....... 
                /* 0010 */  0x21, 0x22, 0x24, 0x28, 0x30, 0x40, 0x41, 0x42,  // !"$(0@AB
                /* 0018 */  0x44, 0x48, 0x50, 0x60, 0x80, 0x81, 0x82, 0x84,  // DHP`....
                /* 0020 */  0x88, 0x90, 0xC0                                 // ...
            })
            Method (ADVD, 0, NotSerialized)
            {
                If (UPDN)
                {
                    UPDN = Zero
                    ^^^LPCB.EC0.SPIN (0x35, Zero)
                    CHA1 = One
                    CHA2 = One
                    CHA3 = Zero
                    Notify (GFX0, 0xF0) // Hardware-Specific
                    While ((CHA2 == One))
                    {
                        Sleep (0x32)
                        If ((CHA3 != One))
                        {
                            CHA2 = Zero
                        }
                    }

                    Sleep (One)
                    ^^^LPCB.EC0.SPIN (0x35, One)
                    If ((CHA3 == One))
                    {
                        ACTD = MD2A (ACTD)
                        NXTD = ACTD /* \_SB_.ACTD */
                        AVLD = MD2A (AVLD)
                    }
                    Else
                    {
                        SETD = ACTD /* \_SB_.ACTD */
                    }
                }

                If ((CHA3 == One))
                {
                    Local0 = Zero
                    Local1 = AVLD /* \_SB_.AVLD */
                    Local2 = Zero
                    While ((Local0 < 0x23))
                    {
                        If ((NXTD == DerefOf (VGAT [Local0])))
                        {
                            Break
                        }

                        Local0++
                    }

                    If ((Local0 < 0x23))
                    {
                        While ((DerefOf (VGAT [Local0]) != Local2))
                        {
                            Local0++
                            If ((Local0 > 0x22))
                            {
                                Local0 = Zero
                            }

                            Local2 = (DerefOf (VGAT [Local0]) & Local1)
                        }
                    }
                    Else
                    {
                        Local0 = Zero
                    }

                    NXTD = DerefOf (VGAT [Local0])
                }
                Else
                {
                    GNDD ()
                    NXTD = SETD /* \_SB_.SETD */
                }

                SETD = NXTD /* \_SB_.PCI0.PEG0.GFX0.NXTD */
                Return (SETD) /* \_SB_.SETD */
            }

            Method (GCDS, 0, NotSerialized)
            {
                If ((CHA3 == One))
                {
                    Return (ACTD) /* \_SB_.ACTD */
                }
                Else
                {
                    Return (GETD ())
                }
            }

            Method (NATK, 0, NotSerialized)
            {
                Return (One)
            }

            Method (UPBL, 0, NotSerialized)
            {
                If (PA3D)
                {
                    Notify (EDPD, 0x86) // Device-Specific
                }
                Else
                {
                    Notify (EDPD, 0x86) // Device-Specific
                }
            }

            Method (DWBL, 0, NotSerialized)
            {
                If (PA3D)
                {
                    Notify (EDPD, 0x87) // Device-Specific
                }
                Else
                {
                    Notify (EDPD, 0x87) // Device-Specific
                }
            }

            Method (GCBL, 1, NotSerialized)
            {
                Arg0 &= 0x7FFFFFFF
                Local0 = Zero
                While ((Local0 < 0x0B))
                {
                    Local1 = DerefOf (PCTG [Local0])
                    If ((Local1 <= Arg0))
                    {
                        Break
                    }

                    Local0++
                }

                If ((Local0 >= 0x0B))
                {
                    Local2 = 0x0A
                }
                Else
                {
                    Local2 = Local0
                }

                Return (Local2)
            }

            Name (GVK0, Buffer (0xD5)
