                        }
                    }

                    If ((Local0 & 0x01000000))
                    {
                        GACD = ((Local0 >> 0x0C) & 0x0FFF)
                        GATD = (Local0 & 0x0FFF)
                        If ((TLSN == Zero))
                        {
                            TLSN = One
                        }

                        TLSN++
                        If ((TLSN > 0x0D))
                        {
                            TLSN = One
                        }
                    }

                    STS4 |= (DHPE << 0x15)
                    STS4 |= (DHPS << 0x14)
                    STS4 |= (TLSN << 0x08)
                    STS4 |= (DKST << 0x05)
